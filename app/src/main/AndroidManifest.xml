<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andr0day.mediacodetest">

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EntryActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MediaCodecActivity" />
        <activity android:name=".MediaMuxerActivity" />
        <activity android:name=".MediaPlayerActivity" />
        <activity android:name=".gl10.p1.G1P1Activity" />
        <activity android:name=".gl10.p2.G1P2Activity" />
        <activity android:name=".gl10.p3.G1P3Activity" />
        <activity android:name=".gl10.p4.G1P4Activity" />
        <activity android:name=".gl10.p5.G1P5Activity"/>
        <activity android:name=".gl10.p6.G1P6Activity"/>
        <activity android:name=".gl20.p1.G2P1Activity"/>
        <activity android:name=".gl20.google.OpenGLES20Activity"/>
    </application>

</manifest>